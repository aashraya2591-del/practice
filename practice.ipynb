    "# Task 1: Check if all numbers in a list are unique\n",
    "def all_unique(numbers):\n",
    "    return len(numbers) == len(set(numbers))\n",
    "\n",
    "# Test the function\n",
    "print(all_unique([1, 2, 3, 4]))  # True\n",
    "print(all_unique([1, 2, 2, 3]))  # False"
   ]
  }
